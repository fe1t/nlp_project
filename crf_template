#Unigrams

#word in range 3
U00:%x[-3,0]
U01:%x[-2,0]
U02:%x[-1,0]
U03:%x[0,0]
U04:%x[1,0]
U05:%x[2,0]
U06:%x[3,0]

#prefix_per
U07:%x[0,4]

#previous is prefix_per
U08:%x[-1,4]

#prefix_org
U09:%x[0,3]

#previous is prefix_org
U10:%x[-1,3]

#dictname
U11:%x[0,2]

#previous in dictname
U12:%x[-1,2]

#commondict
U13:%x[0,1]

#previous in commondict
U14:%x[-1,1]

#word-2 to word-7 is prefix
U20:%x[0,5]


